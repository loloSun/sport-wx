/* pages/quan/quan.wxss */
page{
  overflow: hidden;
}
.all{
  width: 100%;
  height: 100%;
}
swiper{
  width: 100%;
  height: 100%;
}
.items{
  height: 80%;
  /* overflow: visible; */
}
.slideImage{
  width: 100%;
}
.wx-swiper-dots,.xw-swiper-dots-horizontal {
    margin-bottom: 25%;
}
.go{
  width: 350rpx;
  position: absolute;
  bottom: 80rpx;
  left: 50%;
  transform: translate(-50%,0);
}
.summary {
    position: absolute;
    top: 35%;
    left: 8%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    color: #fff;
    width: 86%;
}
.summary .title {
    font-size: 60rpx;
    margin-bottom: 30rpx;
}